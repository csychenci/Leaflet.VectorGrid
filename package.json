{
  "name": "@chencicsy/leaflet.vectorgrid",
  "version": "1.0.5",
  "description": "Display gridded vector data (sliced GeoJSON or protobuf vector tiles) in Leaflet 1.0",
  "main": "dist/Leaflet.VectorGrid.bundled.min.js",
  "types": "dist/Leaflet.VectorGrid.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/Leaflet.VectorGrid.d.ts"
      ]
    }
  },
  "scripts": {
    "release": "standard-version",
    "test": "exit 0",
    "build": "rollup -c rollup.config.js",
    "watch": "rollup -c rollup.config.js --watch",
    "prepublish": "npm run build",
    "lint": "eslint src",
    "lintfix": "eslint --fix src",
    "docs:build": "typedoc",
    "docs:watch": "typedoc --watch",
    "docs:serve": "python3 -m http.server 8081 --directory docs/api",
    "docs:clean": "rm -rf docs/api",
    "docs:dev": "npm run docs:build && npm run docs:serve"
  },
  "keywords": [
    "leaflet",
    "vector",
    "geojson",
    "topojson"
  ],
  "authors": [
    "Iván Sánchez Ortega <ivan@sanchezortega.es>",
    "Per Liedman <per@liedman.net>"
  ],
  "license": "Beerware",
  "repository": {
    "type": "git",
    "url": "https://github.com/csychenci/Leaflet.VectorGrid.git"
  },
  "bugs": {
    "url": "https://github.com/csychenci/Leaflet.VectorGrid/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/leaflet": "^1.9.20",
    "eslint": "^9.34.0",
    "geojson-vt": "=2.2.0",
    "rollup": "^4.48.0",
    "rollup-plugin-buble": "=0.14.0",
    "rollup-plugin-commonjs": "=5.0.5",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-file-as-blob": "=1.1.2",
    "rollup-plugin-node-resolve": "=2.0.0",
    "rollup-plugin-uglify": "=1.0.1",
    "rollup-plugin-web-worker-loader": "^1.7.0",
    "standard-version": "^9.5.0",
    "typedoc": "^0.28.11"
  },
  "dependencies": {
    "@mapbox/vector-tile": "^1.3.0",
    "pbf": "^3.0.2",
    "topojson-client": "^2.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "peerDependencies": {
    "@types/leaflet": "^1.9.20",
    "leaflet": "^1.9.4"
  }
}
